<div class="row">
  <div class="col-md-offset-3 col-md-6">
    <%= form_for [:admin, @distric] do |f| %>
      <%= render "shared/error_messages", object: @distric %>
      <div class="input-group">
        <%= f.label :provincial %>
        <%= f.select :provincial_id, Provincial.all.collect { |p| [ p.name, p.id ] }, class: "form-control form-select-css" %>
      </div>
      

      <%= f.label :name %><br/>
      <%= f.text_field :name, class: "form-control" %>
      
      <div class="actions">
        <%= f.submit "Submit", class: "btn-primary btn-setdefault" %>
      </div>
    <% end %>
  </div>
</div>